<h1> Here are all the Games </h1>


	<% @games.each do |game| %>
	<% if game.user_id = User.last %>
	<h2><li><%= link_to game.title, game_path(game) %></li></h2>
	<%= game.description %>
	<%= link_to "Rate this game", new_game_rating_path(game)%>
	<%end%>
	<br></br>
	<%end%>


<ol>
<div class="center">
<button><%= link_to "Add a Game", new_game_path%></button>
<button><%= link_to "Rate a Game", new_rating_path%></button>
</div></ol>

